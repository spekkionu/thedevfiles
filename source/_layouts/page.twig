{% extends 'default' %}
{% block content_wrapper %}
    <div class="page card">
        <header class="page__header">
            {% block pagetitle %}
            <h1 class="page__title card__title">{{ page.title }}</h1>
            {% endblock %}
        </header>
        <div class="page__body">
            {{ page.blocks.content|raw }}
        </div>
    </div>
{% endblock %}
